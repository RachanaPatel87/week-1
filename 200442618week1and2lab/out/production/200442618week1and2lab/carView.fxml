<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="455.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="CarController">
    <VBox layoutX="117.0" layoutY="106.0" prefHeight="199.0" prefWidth="371.0">
        <GridPane prefHeight="175.0" prefWidth="371.0">
            <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
            </columnConstraints>
            <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            </rowConstraints>
            <Label prefHeight="33.0" prefWidth="60.0" text="Make :">
                <font>
                    <Font size="18.0" />
                </font>
            </Label>
            <Label text="Model :" GridPane.rowIndex="1">
                <font>
                    <Font size="18.0" />
                </font>
            </Label>
            <Label text="Year Of Model :" GridPane.rowIndex="2">
                <font>
                    <Font size="18.0" />
                </font>
            </Label>
            <Label text="Price ( in CN$) :" GridPane.rowIndex="3">
                <font>
                    <Font size="18.0" />
                </font>
            </Label>
            <ComboBox fx:id="makeComboBox" prefHeight="25.0" prefWidth="191.0" promptText="Select maker" GridPane.columnIndex="1" />
            <ComboBox fx:id="modelComboBox" prefHeight="25.0" prefWidth="191.0" promptText="Select Model" GridPane.columnIndex="1" GridPane.rowIndex="1" />
            <Spinner fx:id="yearOfModelSpinner" prefHeight="25.0" prefWidth="189.0" GridPane.columnIndex="1" GridPane.rowIndex="2" />
            <TextField fx:id="priceTextField" promptText="\$20000-100000" GridPane.columnIndex="1" GridPane.rowIndex="3">
                <font>
                    <Font size="18.0" />
                </font>
            </TextField>
        </GridPane>
    </VBox>
    <Label graphicTextGap="55.0" layoutX="233.0" layoutY="31.0" text="Car Selection" textAlignment="CENTER" underline="true">
        <font>
          <Font size="24.0" />
        </font>
    </Label>
    <Button layoutX="251.0" layoutY="333.0" mnemonicParsing="false" onAction="#createButtonPushed" text="Select Car">
        <font>
          <Font size="18.0" />
        </font>
    </Button>
   <Label fx:id="msgLabel" layoutX="286.0" layoutY="403.0" text="Label">
      <font>
         <Font size="18.0" />
      </font></Label>
</AnchorPane>
